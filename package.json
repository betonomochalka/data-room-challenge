{
  "name": "data-room",
  "version": "1.0.0",
  "description": "A comprehensive Data Room web application for secure file and folder management",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python app.py",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "test": "npm run test:frontend",
    "test:frontend": "cd frontend && npm test",
    "install:all": "npm install && cd frontend && npm install",
    "db:generate": "cd old_backend && npx prisma generate",
    "db:push": "cd old_backend && npx prisma db push",
    "db:migrate": "cd old_backend && npx prisma migrate dev",
    "db:studio": "cd old_backend && npx prisma studio"
  },
  "keywords": [
    "data-room",
    "file-management",
    "react",
    "typescript",
    "python",
    "flask",
    "supabase"
  ],
  "author": "Data Room Team",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv-cli": "^11.0.0"
  }
}
